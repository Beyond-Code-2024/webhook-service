{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node with env",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/main.ts",
      "args": ["run", "start"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "envFile": "${workspaceFolder}/../env/webhook-service"
    }
  ]
}
